{
  "expo": {
    "name": "my-blank_2",
    "slug": "my-blank_2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/fondo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/fondo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/fondo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/fondo.png"
    },
    "plugins": [
      "expo-splash-screen"
    ]
  }
}
